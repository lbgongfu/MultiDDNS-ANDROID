<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:id="@+id/user_detail"
    style="?android:attr/textAppearanceLarge" android:layout_width="match_parent"
    android:layout_height="match_parent" android:padding="16dp" android:textIsSelectable="true"
    android:orientation="vertical"
    tools:context="cn.lbgongfu.multiddns.UserDetailFragment">

    <TableLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <View
            android:layout_height="1dip"
            android:background="#B7B7B7" />

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            style="@style/style_table_row">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:text="@string/user_detail_label_username"
                android:id="@+id/textView6"
                android:layout_column="0" />

            <Space
                android:layout_width="@dimen/horizontal_space"
                android:layout_height="@dimen/vertical_space"
                android:layout_column="1" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:id="@+id/text_username"
                android:layout_column="2" />
        </TableRow>

        <View
            android:layout_height="1dip"
            android:background="#B7B7B7" />

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            style="@style/style_table_row">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:text="@string/user_detail_label_password"
                android:id="@+id/textView28"
                android:layout_column="0" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:id="@+id/text_password"
                android:layout_column="2" />
        </TableRow>

        <View
            android:layout_height="1dip"
            android:background="#B7B7B7" />

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            style="@style/style_table_row">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:text="@string/user_detail_label_deadline"
                android:id="@+id/textView7"
                android:layout_column="0" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:id="@+id/text_deadline"
                android:layout_column="2" />
        </TableRow>

        <View
            android:layout_height="1dip"
            android:background="#B7B7B7" />

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            style="@style/style_table_row">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:text="@string/user_detail_label_register_date"
                android:id="@+id/textView8"
                android:layout_column="0" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:id="@+id/text_register_date"
                android:layout_column="2" />
        </TableRow>

        <View
            android:layout_height="1dip"
            android:background="#B7B7B7" />

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            style="@style/style_table_row">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:text="@string/user_detail_label_live_date"
                android:id="@+id/textView9"
                android:layout_column="0" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:id="@+id/text_live_date"
                android:layout_column="2" />
        </TableRow>

        <View
            android:layout_height="1dip"
            android:background="#B7B7B7" />

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            style="@style/style_table_row">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:text="@string/user_detail_label_curr_ip"
                android:id="@+id/textView10"
                android:layout_column="0" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:id="@+id/text_curr_ip"
                android:layout_column="2" />
        </TableRow>

        <View
            android:layout_height="1dip"
            android:background="#B7B7B7" />

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            style="@style/style_table_row">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:text="@string/user_detail_label_parse_span"
                android:id="@+id/textView11"
                android:layout_column="0" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:id="@+id/text_parse_span"
                android:layout_column="2" />
        </TableRow>

        <View
            android:layout_height="1dip"
            android:background="#B7B7B7" />

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            style="@style/style_table_row">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:text="@string/user_detail_label_verify"
                android:id="@+id/textView12"
                android:layout_column="0" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:id="@+id/text_verify"
                android:layout_column="2" />
        </TableRow>

        <View
            android:layout_height="1dip"
            android:background="#B7B7B7" />

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            style="@style/style_table_row">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:text="@string/user_detail_label_redirect"
                android:id="@+id/textView13"
                android:layout_column="0" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:id="@+id/text_redirect"
                android:layout_column="2" />
        </TableRow>

        <View
            android:layout_height="1dip"
            android:background="#B7B7B7" />

    </TableLayout>
</LinearLayout>
